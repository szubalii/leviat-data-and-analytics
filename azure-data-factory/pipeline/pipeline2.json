{
	"name": "pipeline2",
	"properties": {
		"activities": [
			{
				"name": "Lookup1",
				"type": "Lookup",
				"dependsOn": [
					{
						"activity": "Get Metadata1",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlDWSource",
						"sqlReaderQuery": {
							"value": "truncate table @{pipeline().parameters.base_schema_name}.@{pipeline().parameters.base_table_name}\n\nCOPY INTO @{pipeline().parameters.base_schema_name}.@{pipeline().parameters.base_table_name} (\n    [MANDT] 1,\n    [SPRAS] 2,\n    [BWTTY] 3,\n    [BTBEZ] 4,\n    [t_applicationId] default 'test',\n    [t_jobDtm] default '@{utcNow()}'\n)\nFROM 'https://@{pipeline().globalParameters.storageAccountName}.dfs.core.windows.net:443/@{pipeline().parameters.adls_container_name}/pipeline().parameters.directory_path/pipeline().parameters.file_name'\nWITH (\n    IDENTITY_INSERT='OFF',\n    CREDENTIAL=(IDENTITY='Managed Identity'),\n    FILE_TYPE='PARQUET',\n    COMPRESSION='Snappy'\n)\nOPTION (\n    LABEL='ADF Activity ID: @{pipeline().RunId}'\n)\n",
							"type": "Expression"
						},
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"dataset": {
						"referenceName": "ds_syndw",
						"type": "DatasetReference",
						"parameters": {
							"sqlPool": {
								"value": "@pipeline().globalParameters.synapseDatabaseName",
								"type": "Expression"
							}
						}
					}
				}
			},
			{
				"name": "Get Metadata1",
				"type": "GetMetadata",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataset": {
						"referenceName": "ds_adls_parquet_file",
						"type": "DatasetReference",
						"parameters": {
							"container": {
								"value": "@pipeline().parameters.adls_container_name",
								"type": "Expression"
							},
							"directory": {
								"value": "@pipeline().parameters.directory_path",
								"type": "Expression"
							},
							"file": {
								"value": "@pipeline().parameters.file_name",
								"type": "Expression"
							},
							"storageAccount": {
								"value": "@pipeline().globalParameters.storageAccountName",
								"type": "Expression"
							}
						}
					},
					"fieldList": [
						"structure"
					],
					"storeSettings": {
						"type": "AzureBlobFSReadSettings",
						"enablePartitionDiscovery": false
					}
				}
			}
		],
		"parameters": {
			"adls_container_name": {
				"type": "string",
				"defaultValue": "s4h-caa-200"
			},
			"directory_path": {
				"type": "string",
				"defaultValue": "NULL/T149T/Theobald/Table/Full/In/2023/01/09"
			},
			"file_name": {
				"type": "string",
				"defaultValue": "T149T_2023_01_09_09_37_56_050.parquet"
			},
			"base_schema_name": {
				"type": "string",
				"defaultValue": "base_s4h_cax"
			},
			"base_table_name": {
				"type": "string",
				"defaultValue": "T149T"
			}
		},
		"folder": {
			"name": "00 ELT/04 Synapse Ingestion"
		},
		"annotations": []
	}
}