                                        "type": "Expression"
                                    },
                                    "run_id": {
                                        "value": "@pipeline().RunId",
                                        "type": "Expression"
                                    },
                                    "source_layer": {
                                        "value": "@item().source_layer",
                                        "type": "Expression"
                                    },
                                    "file_name": {
                                        "value": "@item().file_name",
                                        "type": "Expression"
                                    },
                                    "adls_container_name": {
                                        "value": "@item().adls_container_name",
                                        "type": "Expression"
                                    },
                                    "target_layer": {
                                        "value": "@item().target_layer",
                                        "type": "Expression"
                                    }
                                }
                            }
                        },
                        {
                            "name": "Set ADLS Container Name",
                            "type": "SetVariable",
                            "dependsOn": [],
                            "userProperties": [],
                            "typeProperties": {
                                "variableName": "adls_container_name",
                                "value": {
                                    "value": "@if(\n    equals(\n        item().layer_id,\n        6\n    ),\n    concat(\n        item().adls_container_name,\n        '-',\n        toLower(pipeline().parameters.s4h_system_id),\n        '-',\n        string(pipeline().parameters.s4h_client_id)\n    ),\n    item().adls_container_name\n)",
                                    "type": "Expression"
                                }
                            }
                        }
                    ]
