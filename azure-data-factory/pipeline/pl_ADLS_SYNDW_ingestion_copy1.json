{
	"name": "pl_ADLS_SYNDW_ingestion_copy1",
	"properties": {
		"activities": [
			{
				"name": "Copy Parquet to Synapse Base",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "ParquetSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"wildcardFolderPath": {
								"value": "@pipeline().parameters.directory_path",
								"type": "Expression"
							},
							"wildcardFileName": "*",
							"enablePartitionDiscovery": false
						}
					},
					"sink": {
						"type": "SqlDWSink",
						"allowPolyBase": true,
						"polyBaseSettings": {
							"rejectValue": 0,
							"rejectType": "value",
							"useTypeDefault": true
						},
						"disableMetricsCollection": false
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "ds_adls_parquet_file",
						"type": "DatasetReference",
						"parameters": {
							"container": {
								"value": "@pipeline().parameters.adls_container_name",
								"type": "Expression"
							},
							"directory": {
								"value": "@pipeline().parameters.directory_path",
								"type": "Expression"
							},
							"file": {
								"value": "@pipeline().parameters.file_name",
								"type": "Expression"
							},
							"storageAccount": {
								"value": "@pipeline().globalParameters.storageAccountName",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "ds_syndw_table",
						"type": "DatasetReference",
						"parameters": {
							"schema": {
								"value": "@pipeline().parameters.base_schema_name",
								"type": "Expression"
							},
							"table": {
								"value": "@pipeline().parameters.base_table_name",
								"type": "Expression"
							},
							"sqlPool": {
								"value": "@pipeline().globalParameters.synapseDatabaseName",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"base_schema_name": {
				"type": "string",
				"defaultValue": "base_tx_halfen_2_dwh"
			},
			"base_table_name": {
				"type": "string",
				"defaultValue": "FACT_SALESTABLE_Archive"
			},
			"directory_path": {
				"type": "string",
				"defaultValue": "FACT_SALESTABLE/Archive/"
			},
			"file_name": {
				"type": "string",
				"defaultValue": "*"
			},
			"adls_container_name": {
				"type": "string",
				"defaultValue": "tx-halfen-2-dwh-prod"
			}
		},
		"variables": {
			"batch_id": {
				"type": "String",
				"defaultValue": "@guid()"
			}
		},
		"folder": {
			"name": "00 ELT/04 Synapse Ingestion"
		},
		"annotations": [],
		"lastPublishTime": "2023-01-03T09:14:22Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}